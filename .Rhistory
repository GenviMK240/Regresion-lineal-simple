# Datos
dist_ncuentas <- data.frame(
distancia = c(1.1, 100.2, 90.3, 5.4, 57.5, 6.6, 34.7, 65.8, 57.9, 86.1),
num_cuentas = c(110, 2, 3, 98, 40, 94, 31, 5, 8, 10)
)
# Ajustar el modelo de regresión lineal
modelo <- lm(num_cuentas ~ distancia, data = dist_ncuentas)
# Predecir el número de cuentas para una distancia de 1.1 km
distancia_nueva <- 1.1
prediccion <- predict(modelo, newdata = data.frame(distancia = distancia_nueva))
# Encontrar el valor observado para esa distancia en tus datos
valor_observado <- dist_ncuentas$num_cuentas[which(dist_ncuentas$distancia == 1.1)]
# Calcular el error
error <- valor_observado - prediccion
# Mostrar el resultado
cat("El número de cuentas predicho para una distancia de 1.1 km es:", prediccion, "\n")
cat("El número de cuentas observado para una distancia de 1.1 km es:", valor_observado, "\n")
cat("El error asociado es:", error, "\n")
