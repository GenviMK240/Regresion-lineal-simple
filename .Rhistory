# Definir los conjuntos de datos
data <- data.frame(
distancia = c(1.1, 100.2, 90.3, 5.4, 57.5, 6.6, 34.7, 65.8, 57.8, 86.1),
num_cuentas = c(110, 2, 6, 98, 40, 94, 31, 5, 8, 10)
)
# Definir el esquema de validación cruzada
control <- trainControl(method = "cv",  # Validación cruzada
number = 5)      # Número de pliegues (folds)
# Entrenar el modelo utilizando validación cruzada
modelo <- train(num_cuentas ~ distancia,  # Modelo de regresión lineal
data = data,             # Conjunto de datos
method = "lm",           # Método de regresión lineal
trControl = control)     # Esquema de validación cruzada
# Ver el resumen de la validación cruzada
print(modelo)
# Plot del modelo
plot(modelo)
